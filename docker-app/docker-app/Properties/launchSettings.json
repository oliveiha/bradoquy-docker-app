{
  "iisSettings": {
    "windowsAuthentication": false, 
    "anonymousAuthentication": true, 
    "iisExpress": {
      "applicationUrl": "$(propertie_appurl)",
      "sslPort": "$(propertie_sslPort)"
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "$(propertie_env)"
      }
    },
    "docker_app": {
      "commandName": "Project",
      "launchBrowser": true,
      "applicationUrl": "$(propertie_docker_url)",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "$(propertie_env)"
      }
    }
  }
}
